<!DOCTYPE site SYSTEM "/dtd/site.dtd">

<site>
	<globalvar name="authid"/>

	<element id="MEMORY_ROLE_ADMIN" file="element/authentication/memory_authenticated_role.xml"/>
	<element id="MEMORY_ROLE_ADMIN_TARGET" file="element/roleuser/target_janino.xml" url="/memory/role_admin" inherits="MEMORY_ROLE_ADMIN"/>

	<element id="MEMORY_ROLE_MAINT" file="element/authentication/memory_authenticated_role2.xml"/>
	<element id="MEMORY_ROLE_MAINT_TARGET" file="element/roleuser/target_janino.xml" url="/memory/role_maint" inherits="MEMORY_ROLE_MAINT"/>
	
	<element id="IDENTIFIED" file="element/roleuser/identified.xml">
		<property name="authElementId">MEMORY_ROLE_ADMIN</property>
	</element>
	<element id="IDENTIFIED_TARGET" file="element/roleuser/target_janino.xml" url="/identified" inherits="IDENTIFIED"/>

	<element file="element/roleuser/explicit_evaluation_janino.xml" url="/explicit_evaluation" inherits="MEMORY_ROLE_ADMIN"/>

	<element id="DATABASE_ROLE_ADMIN" file="element/authentication/database_authenticated_role.xml"/>
	<element id="DATABASE_ROLE_ADMIN_TARGET" file="element/roleuser/target_janino.xml" url="/database/role_admin" inherits="DATABASE_ROLE_ADMIN"/>
	
	<element id="DATABASE_ROLE_MAINT" file="element/authentication/database_authenticated_role2.xml"/>
	<element id="DATABASE_ROLE_MAINT_TARGET" file="element/roleuser/target_janino.xml" url="/database/role_maint" inherits="DATABASE_ROLE_MAINT"/>
	
	<element id="DATABASE_ENCRYPTED" file="element/authentication/database_authenticated_encrypted.xml"/>
	<element id="DATABASE_ENCRYPTED_TARGET" file="element/roleuser/target_janino.xml" url="/database/encrypted" inherits="DATABASE_ENCRYPTED"/>
</site>

