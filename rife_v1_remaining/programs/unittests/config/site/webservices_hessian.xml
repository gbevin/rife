<!DOCTYPE site SYSTEM "/dtd/site.dtd">

<site>
	<element id="BASIC" file="element/webservices/hessian/basic.xml" url="/basic"/>
	<element id="ECHO" file="element/webservices/hessian/echo.xml" url="/echo"/>
	<element id="ELEMENTSERVICE" file="element/webservices/hessian/elementservice.xml" url="/elementservice"/>

	<group>
		<globalvar name="authid"/>
	
		<element id="AUTH" file="element/webservices/hessian/auth.xml"/>

		<element id="AUTHTARGET" file="element/webservices/hessian/auth_target.xml" url="/auth" inherits="AUTH"/>
		<element id="ECHOAUTH" file="element/webservices/hessian/echo.xml" url="/echoauth" inherits="AUTH"/>
	</group>
</site>

